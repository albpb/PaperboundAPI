@model IEnumerable<PaperboundAPI.Models.Qr>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdLlibreNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPuntRecollidaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdLlibreNavigation.IdLlibre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPuntRecollidaNavigation.IdPuntRecollida)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdQr">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdQr">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdQr">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
